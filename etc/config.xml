<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mageinic_cookienotification>
            <general>
                <enabledisable>0</enabledisable>
                <box_position>1</box_position>
                <notice_title>Cookie Notice</notice_title>
                <notice_message>These Websites Use Cookies to ensure you get the best user experience on our sites.
                </notice_message>
                <cookie_policy_url>all</cookie_policy_url>
                <behaviour>1</behaviour>
                <expire_cookie_day>1</expire_cookie_day>
            </general>
        </mageinic_cookienotification>
    </default>
</config>
